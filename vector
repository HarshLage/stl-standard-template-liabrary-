#include <bits/stdc++.h>
using namespace std;
int main()
{
    vector<int> vec;
    vec.push_back(1);
    vec.push_back(2);
    vec.push_back(3);
    vec.push_back(4);
    vec.push_back(5);
    cout << "before pop " << endl;
    //declare size with 5 but intiallize all the elements with 1
    vector<int>a(5,1);


        for (int i = 0; i < a.size(); i++)
    {
        cout << a[i] << " ";
    }

    vector<int>last(a);
    cout<<endl;
    
        for (int i = 0; i < last.size(); i++)
    {
        cout << last[i] << " ";
    }


    cout<<endl;

    for (int i = 0; i < vec.size(); i++)
    {
        cout << vec[i] << " ";
    }
    cout << endl;
    cout << vec.capacity() << endl;

    cout << vec.capacity() << endl;
    cout << vec.size() << endl;
    vec.pop_back();
    cout << "after pop " << endl;
    for (int i = 0; i < vec.size(); i++)
    {
        cout << vec[i] << " ";
    }
        cout<<endl;
        cout << vec.capacity() << endl;
    return 0;
}
