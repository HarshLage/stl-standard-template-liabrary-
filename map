#include<bits/stdc++.h>
using namespace std;
int main()
{
  map<int,string>m;
  m[1] = "harsh";
  m[2] = "ganapati bappa";
  m[3] = "hanuman";

  for(auto i : m){
    cout<< i.first <<" "<< i.second << endl;
  }
  
  
  cout<<" finding 3 -> "<< m.count(3) << endl;

  cout<<"erasing operations"<<endl;
  m.erase(1);
   for(auto i : m){
    cout<< i.first << " " << i.second <<endl;
   }

   auto it = m.find(2);
    for(auto it = m.begin();it!=m.end();++it){
    cout<<(*it).first<<" "<<endl;
  }
}
